{
  "name": "echarts-next-for-react",
  "version": "1.0.7",
  "description": "Echarts(v5.x | next) components for react.",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "authors": {
    "name": "BigFaceMaster",
    "email": "BigFaceMaster@gmail.com",
    "url": "https://github.com/BigFaceMaster"
  },
  "keywords": [
    "react",
    "component",
    "echarts-react",
    "echarts",
    "react-echarts",
    "echarts-next-react",
    "echarts-next",
    "echarts-v5-react",
    "echarts-v5",
    "chart",
    "charts",
    "graph",
    "react-component"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BigFaceMaster/echarts-next-for-react"
  },
  "bugs": {
    "url": "https://github.com/BigFaceMaster/echarts-next-for-react/issues",
    "email": "bigfacemaster@gmail.com"
  },
  "scripts": {
    "lint": "npm run lint:js && npm run lint:prettier && npm run prettier",
    "lint-staged:js": "eslint --ext .ts,.tsx && npm run prettier && npm run lint",
    "lint:fix": "eslint --fix --cache --ext .ts,.tsx --format=pretty ./src",
    "lint:js": "eslint --cache --ext .ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"**/*\"",
    "dev": "father doc dev",
    "build": "father build",
    "start": "dumi dev",
    "site": "dumi build",
    "deploy": "npm run site && gh-pages -d /docs-dist"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "size-sensor": "^1.0.1",
    "echarts": "^5.0.0",
    "echarts-gl": "^2.0.0-rc.3",
    "lodash": "^4.17.20",
    "react-use": "^15.3.4"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/lodash": "^4.14.144",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/fabric": "^2.2.0",
    "cross-env": "^7.0.0",
    "eslint": "^7.1.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.1",
    "father": "^2.16.0",
    "typescript": "^4.0.0",
    "dumi": "next",
    "umi": "^3.2.10"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "npm run lint-staged:js",
    "**/*.{tsx,ts,md,json}": [
      "prettier --write"
    ],
    "*.{tsx,ts,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "license": "MIT"
}
